<main class="col-md-8 ms-sm-auto col-lg-10 px-md-4">
    <div class="table-responsive">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-5">
                        <h2>Clients</h2>
                    </div>
                </div>
            </div>
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>Customer Number</th>
                        <th>Customer Name</th>
                        <th>Phone</th>
                        <th>Country</th>
                        <th>Credit Limit</th>
                        <th>Shipped Products</th>
                    </tr>
                </thead>
                <tbody>
                    <% arrClientes.slice(0, 10).forEach((customer) => { %>  
                        <tr>
                          <td><%= customer.customerNumber %></td>  
                          <td><%= customer.customerName %></td>  
                          <td><%= customer.phone %></td> 
                          <td><%= customer.country %></td>   
                          <td><%= customer.creditLimit %></td>   
                          <td>  
                              <a href="/data/shipped/<%= customer.customerNumber %>" class="settings" title="Settings" data-toggle="tooltip"><i class="material-icons">view list</i></a>   
                          </td>  
                        </tr>  
                      <% }); %>  
                </tbody>
            </table>
        </div>
    </div>  
</main>